@model Warehouse.Models.StockMovementViewModel


<h4 class="py-3 mb-4"><span class="text-muted fw-light">Stock/</span> Details</h4>

<div class="row">
    <div class="col-xl">
        <div class="card mb-4">
            <div class="card-body">
                <dl class="row mb-0">

                    <dt class="col-sm-3">
                        Product ID
                    </dt>
                    <dd class="col-sm-9">
                        @Model.productid
                    </dd>

                    <dt class="col-sm-3">
                        From warehouse ID
                    </dt>
                    <dd class="col-sm-9">
                        @Model.fromwarehouseid
                    </dd>

                    <dt class="col-sm-3">
                        To warehouse ID
                    </dt>
                    <dd class="col-sm-9">
                        @Model.towarehouseid
                    </dd>

                    <dt class="col-sm-3">
                        Quantity
                    </dt>
                    <dd class="col-sm-9">
                        @Model.quantity
                    </dd>

                    <dt class="col-sm-3">
                        Date
                    </dt>
                    <dd class="col-sm-9">
                        @Model.date
                    </dd>




                </dl>
            </div>
        </div>
    </div>
</div>