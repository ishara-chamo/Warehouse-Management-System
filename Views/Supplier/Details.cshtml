@model Warehouse.Models.SupplierViewModel


<h4 class="py-3 mb-4"><span class="text-muted fw-light">Supplier/</span> Details</h4>

<div class="row">
    <div class="col-xl">
        <div class="card mb-4">
            <div class="card-body">
                <dl class="row mb-0">

                    <dt class="col-sm-3">
                        Supplier Name
                    </dt>
                    <dd class="col-sm-9">
                        @Model.SupplierName
                    </dd>

                    <dt class="col-sm-3">
                        Contact Info
                    </dt>
                    <dd class="col-sm-9">
                        @Model.Contactinfo
                    </dd>




                </dl>
            </div>
        </div>
    </div>
</div>