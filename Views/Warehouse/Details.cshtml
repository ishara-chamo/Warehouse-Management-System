@model Warehouse.Models.WarehouseViewModel
<h4 class="py-3 mb-4"><span class="text-muted fw-light">Warehouse/</span> List</h4>

<div class="row">
    <div class="col-xl">
        <div class="card mb-4">
            <div class="card-body">
                <dl class="row mb-0">

                    <dt class="col-sm-3">
                        Warehouse Name
                    </dt>
                    <dd class="col-sm-9">
                        @Model.warename
                    </dd>

                    <dt class="col-sm-3">
                     Location
                    </dt>
                    <dd class="col-sm-9">
                        @Model.location
                    </dd>

                     <dt class="col-sm-3">
                     Contact
                    </dt>
                    <dd class="col-sm-9">
                        @Model.contact
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</div>